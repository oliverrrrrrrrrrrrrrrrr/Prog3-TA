  </xs:complexType>
  <xs:complexType name="contarProductosCarritoResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cancelarOrdenesExpiradas">
    <xs:sequence />
